{
  "version": 2,
  
  "routes": [
     "src": "/api/user/login",
      "methods": ["POST"],
      "dest": "api/user/login.js"
    },
    {
      "src": "/api/user/signup",
      "methods": ["POST"],
      "dest": "api/user/signup.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}

